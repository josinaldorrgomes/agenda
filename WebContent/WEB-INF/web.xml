<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>Agenda</display-name>
	
	<servlet>
		<description></description>
		<display-name>SalvarServlet</display-name>
		<servlet-name>SalvarServlet</servlet-name>
		<servlet-class>br.com.agenda.servlet.SalvarServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SalvarServlet</servlet-name>
		<url-pattern>/Salvar</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<description></description>
		<display-name>ListarServlet</display-name>
		<servlet-name>ListarServlet</servlet-name>
		<servlet-class>br.com.agenda.servlet.ListarServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ListarServlet</servlet-name>
		<url-pattern>/Listar</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<description></description>
		<display-name>EditarServlet</display-name>
		<servlet-name>EditarServlet</servlet-name>
		<servlet-class>br.com.agenda.servlet.EditarServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EditarServlet</servlet-name>
		<url-pattern>/Editar</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<description></description>
		<display-name>ExcluirServlet</display-name>
		<servlet-name>ExcluirServlet</servlet-name>
		<servlet-class>br.com.agenda.servlet.ExcluirServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ExcluirServlet</servlet-name>
		<url-pattern>/Excluir</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>